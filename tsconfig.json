{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "node_modules/@types",
    "node_modules/expo-env.d.ts",
    "src/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
